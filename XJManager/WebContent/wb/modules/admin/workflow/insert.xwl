{"title":"添加","inframe":false,"hidden":false,"roles":{},"pageLink":"","children":[{"configs":{"serverScript":"var id = SysUtil.getId(),\n  data = app.get();\napp.run('select 1 from WB_FLOW where FLOW_NAME={?FLOW_NAME?}', {\n  errorText: '流程 \"' + data.FLOW_NAME + '\" 已经存在。'\n});\napp.set({\n  FLOW_ID: id,\n  MODIFY_DATE: new JavaDate(),\n  MODIFY_USER: data['sys.user']\n});\napp.update({\n  tableName: 'WB_FLOW',\n  mode: 'insert'\n});\napp.output('select FLOW_NAME as \"text\",FLOW_ID,MODIFY_USER,MODIFY_DATE,REMARK from WB_FLOW where FLOW_ID={?FLOW_ID?} order by FLOW_NAME', {\n  type: 'object'\n});","itemId":"module"},"children":[],"type":"module","expanded":false}],"iconCls":""}