{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"initScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\nrequest.setAttribute(\"language\",lang);","loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT tt.*,\ncase when tt.APP_NAME2 is null or tt.APP_NAME2 = '' then tt.APP_NAME else tt.APP_NAME2 end APP_NAME\n FROM (\n  select t.*, t.attribute2 IS_SYS,l.app_lang_val APP_NAME2 \n  from XIP_PUB_APPS t\n  left join xip_pub_app_language l on l.app_code = t.app_code and l.app_lang = '{#language#}' \n) tt\n where tt.APP_CODE like '%{#appCodeName#}%'\n    or tt.APP_NAME2 like '%{#appCodeName#}%'\n order by tt.CREATION_DATE desc","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}