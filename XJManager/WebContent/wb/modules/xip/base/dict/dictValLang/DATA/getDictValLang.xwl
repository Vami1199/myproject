{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"initScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\nrequest.setAttribute(\"language\",lang);","itemId":"module"},"children":[{"configs":{"sql":"SELECT c.VAL_CODE,\n       c.VAL_NAME,\n       c.VCODE,\n       c.VSCODE,\n       g.VAL_CODE CODE,\n       g.VAL_SET_LANG,\n       g.VAL_NAME VNAME,\n       g.DESCRIPTION\n  FROM (select  l.VAL_CODE,\n         l.VAL_NAME,\n         a.VAL_SET_CODE VSCODE,\n         a.VAL_CODE VCODE,\n         a.VAL_NAME VNAME,\n         a.DESCRIPTION  \n   from (select d.VAL_CODE, \n                case when l.VAL_NAME is null or l.VAL_NAME = '' then d.VAL_NAME else l.VAL_NAME end VAL_NAME,\n                d.VAL_SET_CODE VSD\n             from xip_pub_val_set_dtl d\n                  left join XIP_PUB_VAL_SET_DTL_LANG l\n                   on d.VAL_SET_CODE = l.VAL_SET_CODE  and d.VAL_CODE = l.VAL_CODE and l.VAL_SET_LANG = '{#language#}'\n                  where d.VAL_SET_CODE = 'XIP_LANG'\n                  ) l,xip_pub_val_set_dtl a,xip_pub_val_sets s WHERE l.VSD = 'XIP_LANG' and s.VAL_SET_CODE = a.VAL_SET_CODE ) c \n  LEFT JOIN xip_pub_val_set_dtl_lang g \n    ON g.VAL_SET_CODE = c.VSCODE \n   AND g.VAL_SET_LANG = c.VAL_CODE\n   AND g.VAL_CODE = c.VCODE\n WHERE c.VAL_CODE like '%{#lang#}%'\n   AND c.VSCODE like '%{#val_set_code#}%'\n ORDER BY c.VSCODE asc , c.VAL_CODE asc\n","itemId":"dataprovider"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}