{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","serverScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\nrequest.setAttribute(\"xipLanguage\",lang);","itemId":"module"},"children":[{"configs":{"itemId":"sqlswitcher1","oracle":"SELECT VT.PARAM_ID,\n       VT.EF ENABLE_FLAG,\n       DECODE(VT.VSC,null,VT.VAL,VT.VAL) VAL,\n       DECODE(VT.VSC,null,null,VT.VAL) VAL_CODE,\n       VT.PARAM_NAME,\n       VT.VSC VAL_SET_CODE,\n       VT.VAL_SQL,\n       VT.ROLE_NAME,\n       VT.TARGET_ID,\n       VT.PARAM_VAL_ID\n FROM (SELECT * FROM (SELECT X1.PARAM_VAL_ID,\n                            X1.PARAM_ID,\n                            X1.TARGET_ID,\n                            (SELECT XPA.ROLE_NAME FROM XIP_PUB_ROLE XPA WHERE XPA.ROLE_ID=X1.TARGET_ID) ROLE_NAME,\n                            X1.VAL,\n                            X1.VAL_LEVEL,\n                            X1.ENABLE_FLAG EF,\n                            X2.VAL_SET_CODE VSC,\n                            X2.VAL_SQL,\n                            case when (NVL(l.PARAM_NAME, '')=null or l.PARAM_NAME is null) then X2.PARAM_NAME else l.PARAM_NAME end PARAM_NAME\n                       FROM XIP_PUB_PARAM_VALS X1 left join XIP_PUB_PARAMS X2 on X1.PARAM_ID=X2.PARAM_ID LEFT JOIN xip_pub_param_language l ON X2.PARAM_CODE = l.PARAM_CODE AND l.LANG_CODE = {?xipLanguage?}) T \n      ) VT\nWHERE VT.VAL_LEVEL='R' AND VT.PARAM_ID={?PARAM_ID?}","mysql":"SELECT VT.PARAM_ID,\n       VT.EF ENABLE_FLAG,\n       IF(VT.VSC is null,VT.VAL,VT.VAL) VAL,\n       IF(VT.VSC is null,null,VT.VAL) VAL_CODE,\n       VT.PARAM_NAME,\n       VT.VSC VAL_SET_CODE,\n       VT.VAL_SQL,\n       VT.ROLE_NAME,\n       VT.TARGET_ID,\n       VT.PARAM_VAL_ID\n FROM (SELECT * FROM (SELECT X1.PARAM_VAL_ID,\n                            X1.PARAM_ID,\n                            X1.TARGET_ID,\n                            (SELECT XPA.ROLE_NAME FROM XIP_PUB_ROLE XPA WHERE XPA.ROLE_ID=X1.TARGET_ID) ROLE_NAME,\n                            X1.VAL,\n                            X1.VAL_LEVEL,\n                            X1.ENABLE_FLAG EF,\n                            X2.VAL_SET_CODE VSC,\n                            X2.VAL_SQL,\n                            CASE WHEN (l.PARAM_NAME = NULL OR l.PARAM_NAME = '' OR l.PARAM_NAME IS NULL) THEN X2.PARAM_NAME ELSE l.PARAM_NAME END PARAM_NAME\n                       FROM XIP_PUB_PARAM_VALS X1 left join XIP_PUB_PARAMS X2 on X1.PARAM_ID=X2.PARAM_ID LEFT JOIN xip_pub_param_language l ON X2.PARAM_CODE = l.PARAM_CODE AND l.LANG_CODE = {?xipLanguage?}) T \n      ) VT\nWHERE VT.VAL_LEVEL='R' AND VT.PARAM_ID={?PARAM_ID?}"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}