{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"initScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\nrequest.setAttribute(\"language\",lang);","itemId":"module"},"children":[{"configs":{"sql":"SELECT c.VAL_CODE,\n       c.VAL_NAME,\n       c.PARAM_CODE,\n       g.PARAM_CODE CODE,\n       g.LANG_CODE,\n       g.PARAM_NAME,\n       g.DESCRIPTION\n  FROM (select VAL_CODE,\n               VAL_NAME, \n               b.PARAM_CODE,\n               b.PARAM_NAME\n          from (SELECT l.VAL_CODE,\n                       case when t.VAL_NAME is null or t.VAL_NAME = '' then l.VAL_NAME else t.VAL_NAME end VAL_NAME\n                  FROM xip_pub_val_set_dtl l\n                  left join XIP_PUB_VAL_SET_DTL_LANG t \n                         on l.VAL_SET_CODE = t.VAL_SET_CODE  \n                        and l.VAL_CODE = t.VAL_CODE and t.VAL_SET_LANG = '{#language#}'\n                 WHERE l.VAL_SET_CODE = 'XIP_LANG') p, xip_pub_params b) c \n  LEFT JOIN xip_pub_param_language g \n    ON g.PARAM_CODE = c.PARAM_CODE \n   AND g.LANG_CODE = c.VAL_CODE\n WHERE c.VAL_CODE like '%{#lang#}%'\n   AND (c.PARAM_CODE like '%{#paramCodeName#}%' or c.PARAM_NAME like '%{#paramCodeName#}%')\n ORDER BY c.PARAM_CODE asc , c.VAL_CODE asc","itemId":"dataprovider"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}