{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"SELECT server_id,\n       server_name,\n       server_type,\n       host,\n       port, \n       web_project,\n       username,\n       password,\n       enable_flag,\n       server_addr\n  FROM (SELECT t.server_id,\n               t.server_name,\n               t.server_type,\n               t.host,\n               t.port,\n               t.web_project,\n               t.username,\n               t.password,\n               t.enable_flag,\n               t.CREATION_DATE,\n               t.host || ':' || t.port || '/' ||\n               t.web_project server_addr\n          FROM xip_pub_att_server t)\n           WHERE SERVER_ADDR LIKE '%{#serverHost#}%'\n             AND SERVER_NAME LIKE '%{#serverName#}%'\n             AND ENABLE_FLAG LIKE '%{#quztCombox#}%'\n           ORDER BY CREATION_DATE DESC\n","itemId":"sqlswitcher1","mysql":"SELECT a.* FROM (\nSELECT         t.SERVER_ID,\n               t.SERVER_NAME,\n               t.SERVER_TYPE,\n               t.HOST,\n               t.PORT,\n               t.WEB_PROJECT,\n               t.USERNAME,\n               t.PASSWORD,\n               t.ENABLE_FLAG,\n               t.CREATION_DATE,\n               CONCAT(t.host,':',t.port,'/',CASE  WHEN t.web_project IS NULL THEN '' ELSE t.web_project END) SERVER_ADDR\n          FROM xip_pub_att_server t ) a\n           WHERE (a.SERVER_ADDR LIKE '%{#serverHost#}%' OR a.SERVER_ADDR  IS NULL)\n             AND (a.SERVER_NAME LIKE '%{#serverName#}%' OR a.SERVER_NAME  IS NULL)\n             AND (a.ENABLE_FLAG LIKE '%{#quztCombox#}%'  OR a.ENABLE_FLAG  IS NULL)\n           ORDER BY CREATION_DATE DESC"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","ico