{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","serverScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\nrequest.setAttribute(\"language\",lang);","itemId":"module"},"children":[{"configs":{"sql":"SELECT \n   a.CONS_ID,\n   a.COL_ID,\n   a.TAB_ID,\n   a.TAB_NAME,\n   a.APP_ID,\n   a.COL_NAME AS \"CONS_COL_NAME\",\n   a.CONS_TYPE,\n   (SELECT VAL_NAME FROM  xip_pub_val_set_dtl WHERE VAL_SET_CODE ='DB_CONS_TYPE' AND VAL_CODE= a.CONS_TYPE) AS \"CONS_TYPE_NAME_BAK\",\n   (select case when l.VAL_NAME is null or l.VAL_NAME = '' then d.VAL_NAME else l.VAL_NAME end VAL_NAME\n     from xip_pub_val_set_dtl d\n     left join XIP_PUB_VAL_SET_DTL_LANG l\n            on d.VAL_SET_CODE = l.VAL_SET_CODE  \n           and d.VAL_CODE = l.VAL_CODE \n           and l.VAL_SET_LANG = '{#language#}'\n    where d.VAL_SET_CODE ='DB_CONS_TYPE'\n      and d.VAL_CODE = a.CONS_TYPE) AS \"CONS_TYPE_NAME\",\n   a.CONS_NAME, \n   a.REF_TAB_ID,\n   a.REF_TAB_NAME AS \"REF_TAB_NAME\",\n   a.REF_TAB_COL\nFROM xip_db_all_tab_constraints a \nWHERE a.CONS_TYPE LIKE '%KEY%' \nAND  a.TAB_ID ={?tableId?}\n\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}