{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","serverScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\nrequest.setAttribute(\"language\",lang);","itemId":"module"},"children":[{"configs":{"sql":"SELECT \n   a.TAB_ID,\n   a.COL_ID,\n   a.COL_NAME,\n   a.TAB_NAME,\n   a.COL_TYPE,\n   a.COL_TYPE AS \"COL_TYPE_NAME\",\n   a.COL_LENGTH,\n   a.COL_COMMENT,\n   a.DEFAULT_VAL,\n   a.IS_NULL,\n   a.COL_SEQ,\n  (SELECT VAL_NAME FROM  xip_pub_val_set_dtl WHERE VAL_SET_CODE ='YN' AND VAL_CODE= a.IS_NULL) AS \"IS_NULL_NAME_BAK\",\n  (select case when l.VAL_NAME is null or l.VAL_NAME = '' then d.VAL_NAME else l.VAL_NAME end VAL_NAME\n     from xip_pub_val_set_dtl d\n     left join XIP_PUB_VAL_SET_DTL_LANG l\n            on d.VAL_SET_CODE = l.VAL_SET_CODE  \n           and d.VAL_CODE = l.VAL_CODE \n           and l.VAL_SET_LANG = '{#language#}'\n    where d.VAL_SET_CODE ='YN'\n      and d.VAL_CODE = a.IS_NULL) AS \"IS_NULL_NAME\",\n   a.APP_ID,\n   '_modified' AS \"OP_TYPE\"\nFROM  xip_db_all_tab_cols a   \nWHERE a.TAB_ID ={?tableId?}\nORDER BY a.COL_SEQ+0 ASC","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}