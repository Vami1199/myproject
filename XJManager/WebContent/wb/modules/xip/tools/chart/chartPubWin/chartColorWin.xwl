{"inframe":false,"title":"图表颜色属性窗口","hidden":false,"roles":{"default":1},"children":[{"configs":{"itemId":"module"},"events":{"initialize":"/**\n *参数说明\n *title：窗口的标题\n *winType:窗口类型，单颜色/多颜色系列(single/multiply)\n */\nvar selectColor ='';\nExt.apply(app,{\n  initWindow:function(params){\n    Ext.apply(app, params);\n    app.chartColorWin = new Ext.window.Window(app.chartColorWin);\n    app.chartColorWin.setTitle(params.title);\n    var defaultColor = '993300';\n    if(!Ext.isEmpty(params.defaultVal)){\n      defaultColor = params.defaultVal.substring(1, params.defaultVal.length-1);\n    }\n    var colorPicker = Ext.create('Ext.picker.Color', {\n      value: defaultColor,\n      listeners: {\n        select: function(picker, selColor) {\n                 selectColor= '#'+selColor;\n        }\n      }\n    });\n    app.chartColorWin.add(colorPicker);\n    app.chartColorWin.show();\n  }\n});"},"children":[{"configs":{"itemId":"winButtons"},"children":[{"configs":{"text":"清除颜色","itemId":"clearColorBtn","iconCls":"wand_icon"},"events":{"click":"selectColor = '';\napp.chartColorWin.fireEvent('ok');"},"children":[],"expanded":true,"type":"button"},{"configs":{"text":"确定","itemId":"confirmBtn","iconCls":"ok_icon"},"events":{"click":"app.chartColorWin.fireEvent('ok');"},"children":[],"expanded":false,"type":"button"},{"configs":{"text":"取消","itemId":"cancelBtn","iconCls":"cancel_icon"},"events":{"click":"app.chartColorWin.close();"},"children":[],"expanded":false,"type":"button"}],"expanded":true,"type":"array"},{"configs":{"height":"150","width":"300","layout":"fit","resizable":"true","buttons":"app.winButtons","buttonAlign":"center","html":"<!-- <body id='htmlBody'>\n  <div id='ddiv' style=\"width:300px\">\n     <input type='color' id='colorT' style=\"width:300px\" value=''/>\n  <\/div>\n<\/body> -->","dialog":"true","itemId":"chartColorWin","createInstance":"false"},"events":{"ok":"//返回弹出框的回带值\nvar returnObj ={'name':'','code':''};\nif(Ext.isEmpty(selectColor)){\n  returnObj.name='';\n  returnObj.code='';\n}else{\n  returnObj.code = selectColor;\n  returnObj.name = selectColor;\n}\nif (app.success)\n  app.success(returnObj);\napp.chartColorWin.close();"},"children":[],"expanded":true,"type":"window"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"color_icon"}