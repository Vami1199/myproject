{"inframe":false,"title":"图表属性编辑窗口","hidden":false,"roles":{"default":1},"children":[{"configs":{"serverScript":"com.xzsoft.xip.platform.util.XipUtil.getCurrentLang(request,\"xip/tools/chart/portalChartCommon\");","itemId":"module"},"events":{"initialize":"/**\n *参数说明\n *title：窗口的标题\n *data ：下拉框store的数据\n *fieldLabel：下拉框的标题(默认为'属性值')\n */\nExt.apply(app,{\n  initWindow:function(params){\n    Ext.apply(app, params);\n    app.chartAttrWin = new Ext.window.Window(app.pubWin);\n    app.chartAttrWin.setTitle(params.title);\n    app.attrCombobox.setFieldLabel(params.fieldLabel);\n    var jsonDataArray = Wb.decode(params.data);\n    var _empJson = {name:\"none\",value:\"---\"};\n    jsonDataArray.splice(0,0,_empJson);\n    app.attrCombobox.store.loadRawData(jsonDataArray);\n    app.chartAttrWin.show();\n  }\n});"},"children":[{"configs":{"title":"{# lang_chart_attr_win_title#}","height":"150","constrain":"true","width":"300","layout":"fit","border":"false","dialog":"true","itemId":"pubWin"},"events":{"ok":"//返回弹出框的回带值\nvar returnObj ={'name':'','code':''};\nif(Ext.isEmpty(app.attrCombobox.getValue()) || app.attrCombobox.getValue() === 'none'){\n  returnObj.name='';\n  returnObj.code='';\n}else{\n  returnObj.code = app.attrCombobox.getValue();\n  returnObj.name = app.attrCombobox.getRawValue();\n}\nif (app.success)\n  app.success(returnObj);\napp.pubWin.close();"},"children":[{"configs":{"frame":"true","style":"border-width:0px","layout":"form","border":"false","itemId":"attrForm"},"children":[{"configs":{"labelWidth":"100","queryMode":"local","displayField":"value","valueField":"name","labelAlign":"left","itemId":"attrCombobox","fieldLabel":"Value"},"children":[{"configs":{"pageSize":"-1","itemId":"store","fields":"[{\"name\":\"name\",\"type\":\"string\"},{\"name\":\"value\",\"type\":\"string\"}]"},"children":[],"expanded":false,"type":"store"}],"expanded":true,"type":"combo"}],"expanded":true,"type":"form"}],"expanded":true,"type":"window"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"textarea_icon"}