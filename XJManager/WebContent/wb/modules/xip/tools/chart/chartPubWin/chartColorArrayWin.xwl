 = document.getElementById('colorT').value;\nif(Ext.isEmpty(firstColor)){\n  returnObj.name='';\n  returnObj.code='';\n}else{\n  var _addDiv = document.getElementById('otherDiv');\n  var _colorInputs = _addDiv.getElementsByTagName('input');\n  for(var i=0;i<_colorInputs.length;i++){\n    if(i === _colorInputs.length-1){\n      firstColor +=_colorInputs[i].value;\n    }else{\n      firstColor +=_colorInputs[i].value+',';\n    }\n  }\n  returnObj.code = firstColor;\n  returnObj.name = firstColor;\n}\nif (app.success)\n  app.success(returnObj);\napp.chartColorArrayWin.close();"},"children":[],"expanded":true,"type":"window"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"color_icon"}