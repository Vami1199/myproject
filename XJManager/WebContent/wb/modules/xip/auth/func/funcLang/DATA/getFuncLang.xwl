{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"initScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\nrequest.setAttribute(\"language\",lang);","itemId":"module"},"children":[{"configs":{"sql":"SELECT c.VAL_CODE,\n       c.VAL_NAME,\n       c.FUN_TREE_CODE,\n       g.FUN_CODE,\n       g.FUN_LANG,\n       g.FUN_LANG_VAL\n  FROM (select  VAL_CODE,\n      VAL_NAME, \n      a.FUN_TREE_CODE \n      from \n      (SELECT l.VAL_CODE,\n       case when t.VAL_NAME is null or t.VAL_NAME = '' then l.VAL_NAME else t.VAL_NAME end VAL_NAME\n\n       FROM xip_pub_val_set_dtl l\n\n       left join XIP_PUB_VAL_SET_DTL_LANG t \n       on l.VAL_SET_CODE = t.VAL_SET_CODE  and l.VAL_CODE = t.VAL_CODE and t.VAL_SET_LANG = '{#language#}'\n       WHERE l.VAL_SET_CODE = 'XIP_LANG') p, xip_pub_fun_tree a ) c \n  LEFT JOIN xip_pub_fun_language g \n    ON g.FUN_CODE = c.FUN_TREE_CODE \n   AND g.FUN_LANG = c.VAL_CODE\n WHERE c.VAL_CODE like '%{#lang#}%'\n   AND (c.FUN_TREE_CODE like '%{#funCode#}%' or g.FUN_LANG_VAL like '%{#funCode#}%')\n ORDER BY c.FUN_TREE_CODE asc , c.VAL_CODE asc","itemId":"dataprovider"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}