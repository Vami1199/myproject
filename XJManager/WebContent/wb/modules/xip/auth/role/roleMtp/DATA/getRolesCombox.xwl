{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"initScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\nrequest.setAttribute(\"language\",lang);","loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"select t.ROLE_CODE,\n       case when l.ROLE_NAME_LANG_VAL is null or l.ROLE_NAME_LANG_VAL = '' then t.ROLE_NAME else l.ROLE_NAME_LANG_VAL end \"ROLE_NAME\",\n       t.MNU_GROUP_ID \n from xip_pub_role t\n left join xip_pub_role_language l on l.ROLE_CODE = t.ROLE_CODE and l.ROLE_LANG = {?language?}\nwhere( t.ROLE_ID  in (select l.ROLE_ID from  xip_pub_user_roles l where   l.USER_ID = {?XIP.userId?})  \n       or\n      t.CREATED_BY ={?XIP.userId?})","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}