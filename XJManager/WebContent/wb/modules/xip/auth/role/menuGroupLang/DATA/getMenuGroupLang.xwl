{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"initScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\nrequest.setAttribute(\"language\",lang);","itemId":"module"},"children":[{"configs":{"sql":"SELECT c.VAL_CODE,\n       c.VAL_NAME,\n       c.MNU_TREE_CODE,\n       c.MNU_ID,\n       c.MNU_GROUP_ID,\n       g.FUN_CODE,\n       g.FUN_LANG,\n       g.FUN_LANG_VAL,\n       c.ROLE_CODE,\n       c.ROLE_NAME\n  FROM  (SELECT l.VAL_CODE,\n               l.VAL_NAME,\n               t.MNU_TREE_CODE,\n               t.MNU_ID,\n               t.MNU_GROUP_ID,\n               r.ROLE_CODE,\n               r.ROLE_NAME\n          FROM (select d.VAL_CODE, \n                case when l.VAL_NAME is null or l.VAL_NAME = '' then d.VAL_NAME else l.VAL_NAME end VAL_NAME,\n                d.VAL_SET_CODE VSD\n             from xip_pub_val_set_dtl d\n                  left join XIP_PUB_VAL_SET_DTL_LANG l\n                   on d.VAL_SET_CODE = l.VAL_SET_CODE  and d.VAL_CODE = l.VAL_CODE and l.VAL_SET_LANG = '{#language#}'\n                  where d.VAL_SET_CODE = 'XIP_LANG'\n                  ) l, xip_pub_mnu_tree t, xip_pub_role r\n         WHERE l.VSD = 'XIP_LANG' \n           AND r.MNU_GROUP_ID = t.MNU_GROUP_ID\n {#roleCondition#} ) c  \n  LEFT JOIN xip_pub_menugroup_language g \n    ON g.FUN_CODE = c.MNU_TREE_CODE \n   AND g.FUN_LANG = c.VAL_CODE\n   AND g.MNU_GROUP_ID = c.MNU_GROUP_ID\n WHERE 1=1 {#conditions#}\n ORDER BY c.MNU_TREE_CODE ASC , c.VAL_CODE ASC","itemId":"dataprovider"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}