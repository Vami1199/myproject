{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"initScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\nrequest.setAttribute(\"language\",lang);","loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"select t.ROLE_ID,\n       t.ROLE_CODE,\n       case when l.ROLE_NAME_LANG_VAL is null or l.ROLE_NAME_LANG_VAL = '' then t.ROLE_NAME else l.ROLE_NAME_LANG_VAL end \"ROLE_NAME\",\n       case when l.ROLE_DESC_LANG_VAL is null or l.ROLE_DESC_LANG_VAL = '' then t.DESCRIPTION else l.ROLE_DESC_LANG_VAL end \"DESCRIPTION\",\n       t.MNU_GROUP_ID,\n       t.ENABLE_FLAG\n  from xip_pub_role t\n  left join xip_pub_role_language l on l.ROLE_CODE = t.ROLE_CODE and l.ROLE_LANG = {?language?}\n where t.role_id <> {?sourceRoleId?}\n   and t.role_name like '%{#syncRoleName#}%'\n order by t.CREATION_DATE desc","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}