{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"initScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\n\nrequest.setAttribute(\"language\",lang);","loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"select t.up_id \"parent_id\",\n       case when t.mnu_id is null  then '-1' else t.mnu_id end id,\n       case when t.FUN_TREE_ID is null  \n            then case when l.FUN_LANG_VAL is null  \n                      then t.name\n                      else l.FUN_LANG_VAL\n                      end\n            else case when l.FUN_LANG_VAL is null \n                      then case when (select fl.FUN_LANG_VAL from xip_pub_fun_language fl where fl.FUN_CODE = t.MNU_TREE_CODE and fl.FUN_LANG = {?language?}) is null\n                                then (select f.FUN_TREE_NAME from xip_pub_fun_tree f where f.FUN_TREE_CODE = t.MNU_TREE_CODE)\n                                else (select fl.FUN_LANG_VAL from xip_pub_fun_language fl where fl.FUN_CODE = t.MNU_TREE_CODE and fl.FUN_LANG = {?language?})\n                                end \n                      else l.FUN_LANG_VAL\n                      end\n            end \"text\",\n       t.node_type node_type,\n       t.node_order idx,\n       t.fun_tree_id,\n       t.mnu_group_id,\n       t.MNU_TREE_CODE,\n        (select count(*)\n                       from xip_pub_mnu_tree t1\n                      where t1.up_id = t.mnu_id ) as CHILDCOUNT, \n       case\n         when (select count(*)\n                 from xip_pub_mnu_tree t1\n                where t1.up_id = t.mnu_id) > 0 or t.node_type='M'then\n          'false'\n         else\n          'true'\n       end as \"leaf\",\n       'MNU_TREE' as SRC_CODE\n  from xip_pub_mnu_tree t\nleft join xip_pub_menugroup_language l on l.mnu_id = t.mnu_id and l.MNU_GROUP_ID = t.MNU_GROUP_ID and l.FUN_LANG = {?language?}\n where t.up_id = {?parentID?} or ({?parentID?} is null and t.up_id = '-1')\n   and t.mnu_group_id = {?menuGroupId2?}\n order by t.node_order asc\n\n","itemId":"sqlswitcher1","mysql":"  select t.up_id \"parent_id\",\n       case when t.mnu_id is null or t.mnu_id = '' then '-1' else t.mnu_id end ID,\n       case when t.FUN_TREE_ID is null or t.FUN_TREE_ID = '' \n            then case when l.FUN_LANG_VAL is null or l.FUN_LANG_VAL = '' \n                      then t.name\n                      else l.FUN_LANG_VAL\n                      end\n            else case when l.FUN_LANG_VAL is null or l.FUN_LANG_VAL = '' \n                      then case when IFNULL((select fl.FUN_LANG_VAL from xip_pub_fun_language fl where fl.FUN_CODE = t.MNU_TREE_CODE and fl.FUN_LANG = {?language?}), '') = ''\n                                then t.name\n                                else (select fl.FUN_LANG_VAL from xip_pub_fun_language fl where fl.FUN_CODE = t.MNU_TREE_CODE and fl.FUN_LANG = {?language?})\n                                end \n                      else l.FUN_LANG_VAL\n                      end\n            end \"text\",\n       t.node_type NODE_TYPE,\n       t.node_order IDX,\n       t.FUN_TREE_ID,\n       t.MNU_GROUP_ID,\n       t.MNU_TREE_CODE,\n       (select count(*)\n                       from xip_pub_mnu_tree t1\n                      where t1.up_id = t.mnu_id ) as CHILDCOUNT,  \n       case\n         when (select count(*)\n                 from xip_pub_mnu_tree t1\n                where t1.up_id = t.mnu_id) > 0  or t.node_type='M' then\n          'false'\n         else\n          'true'\n       end as \"leaf\",\n       'MNU_TREE' as SRC_CODE\n  from xip_pub_mnu_tree t\n  left join xip_pub_menugroup_language l on l.mnu_id = t.mnu_id and l.MNU_GROUP_ID = t.MNU_GROUP_ID and l.FUN_LANG = {?language?}\n where t.up_id = {?parentID?} or (({?parentID?} is null OR {?parentID?}='') and IFNULL(t.up_id,'-1') = '-1')\n   and t.mnu_group_id = {?menuGroupId2?}\n order by t.node_order asc\n"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","type":"tree","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}