{"inframe":false,"title":"获取通知","hidden":true,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"select xn.NOTICE_ID MSG_ID,\n       xn.NOTICE_TITLE MSG_TITLE,\n       xn.NOTICE_CONTENT MSG_CONTENT,\n       xn.FORM_URL MSG_URL,\n       xn.SRC_ID,\n       xn.SRC_CAT,\n       xun.USER_NOTICE_ID,\n       xun.USER_ID,\n       xun.STATUS MSG_STATUS,\n       xu.USER_NAME,\n       xe.EMP_CODE,\n       xe.EMP_NAME,\n       xn.created_by CREATED_BY,\n       xn.creation_date CREATION_DATE\n  from xip_pub_notices      xn,\n       xip_pub_user_notices xun,\n       xip_pub_users        xu,\n       xip_pub_emps         xe\n where xn.NOTICE_ID = xun.NOTICE_ID\n   and xun.USER_ID = xu.USER_ID\n   and xu.EMP_ID = xe.EMP_ID\n   and xun.STATUS = '{#status#}'\n   and xu.USER_ID = {?XIP.userId?}\n   and xn.NOTICE_TITLE like '%{#txtMsgTitle#}%'\norder by xn.creation_date desc","itemId":"getNoticeList"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}