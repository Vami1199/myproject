{"inframe":false,"title":"获取角色","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT \n  ur.USER_ROLE_ID,\n  ur.USER_ID,\n  ur.ROLE_ID,\n  ur.ISDEFAUT,\n  ur.ORDERBY,\n  r.ROLE_CODE,\n  r.ROLE_NAME \nFROM\n  xip_pub_user_roles ur,\n  xip_pub_role r \nWHERE ur.enable_flag = 'Y' \n  AND ur.user_id = {?userId?} \n  AND ur.role_id = r.role_id \n\nUNION\n\nSELECT \n  '' USER_ROLE_ID,\n  {?userId?} USER_ID,\n  r.role_id ROLE_ID,\n  'N' ISDEFAUT,\n  '20' ORDERBY,\n  r.role_code ROLE_CODE,\n  r.role_name ROLE_NAME \nFROM\n  xip_pub_role r \nWHERE r.ATTRIBUTE1 = 'S' \n  AND \n  (SELECT \n    u.user_type \n  FROM\n    xip_pub_users u \n  WHERE u.user_id = {?userId?}) = 'S' ","itemId":"dataprovider"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}