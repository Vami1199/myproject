{"inframe":false,"title":"获取员工分配信息列表（增量）","hidden":false,"roles":{"default":1},"children":[{"configs":{"itemId":"module"},"children":[{"configs":{"oracle":"SELECT\nt.ASG_ID as asgId,\nt.EMP_ID as empId,\nt.POST_ID as postId,\nt.DEPT_ID as deptId,\nt.ORG_ID as orgId,\nt.ENABLED as enabled,\nto_char(t.LAST_UPDATE_DATE,'yyyy-mm-dd H24:mi:ss') as lastUpdateDate,\nt.LAST_UPDATED_BY as lastUpdatedBy,\nto_char(t.CREATION_DATE,'yyyy-mm-dd H24:mi:ss') as creationDate,\nt.CREATED_BY as createdBy,\nt.LAST_UPDATE_LOGIN as lastUpdateLogin,\nt.POST_SEQ as postSeq\nFROM\n\txip_pub_emp_asg t\nWHERE CREATION_DATE >= to_date({#lastEtlDate#}, 'yyyymmddhh24miss')\n   OR LAST_UPDATE_DATE >= to_date({#lastEtlDate#}, 'yyyymmddhh24miss')","itemId":"sqlswitcher","mysql":"SELECT\nt.ASG_ID as asgId,\nt.EMP_ID as empId,\nt.POST_ID as postId,\nt.DEPT_ID as deptId,\nt.ORG_ID as orgId,\nt.ENABLED as enabled,\nDATE_FORMAT(t.LAST_UPDATE_DATE,'%Y-%m-%d %H:%i:%s') as lastUpdateDate,\nt.LAST_UPDATED_BY as lastUpdatedBy,\nDATE_FORMAT(t.CREATION_DATE,'%Y-%m-%d %H:%i:%s') as creationDate,\nt.CREATED_BY as createdBy,\nt.LAST_UPDATE_LOGIN as lastUpdateLogin,\nt.POST_SEQ as postSeq\nFROM\n\txip_pub_emp_asg t\nWHERE CREATION_DATE >= DATE_FORMAT({#lastEtlDate#}, '%Y%m%d%H%i%s')\n   OR LAST_UPDATE_DATE >= DATE_FORMAT({#lastEtlDate#}, '%Y%m%d%H%i%s')"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"autoPage":"false","sql":"{#sqlswitcher#}","createColumns":"false","itemId":"dataprovider1","createKeyValues":"false"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}