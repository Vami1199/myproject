{"inframe":false,"title":"获取部门列表(增量)","hidden":false,"roles":{"default":1},"children":[{"configs":{"itemId":"module"},"children":[{"configs":{"oracle":"SELECT\nd.DEPT_ID as deptId,\nd.DEPT_CODE as deptCode,\nd.DEPT_NAME as deptName,\nd.DEPT_SHORT_NAME as deptShortName,\nd.UP_DP_ID as upDpId,\nd.ORG_ID as orgId,\nd.ENABLE_FLAG as enableFlag,\nd.ATTRIBUTE1 as attribute1,\nd.ATTRIBUTE2 as attribute2,\nd.ATTRIBUTE3 as attribute3,\nd.ATTRIBUTE4 as attribute4,\nd.ATTRIBUTE5 as attribute5,\nd.ATTRIBUTE6 as attribute6,\nd.ATTRIBUTE7 as attribute7,\nd.ATTRIBUTE8 as attribute8,\nd.ATTRIBUTE9 as attribute9,\nd.ATTRIBUTE10 as attribute10,\nto_char(d.CREATION_DATE,'yyyy-mm-dd h24:mi:ss') as creationDate,\nd.CREATED_BY as createdBy,\nto_char(d.LAST_UPDATE_DATE,'yyyy-mm-dd h24:mi:ss') as lastUpdateDate,\nd.LAST_UPDATED_BY as lastUpdatedBy,\nd.LAST_UPDATE_LOGIN as lastUpdateLogin,\nd.SEQ_UPDEPT as seqUpdept,\nd.TAG_ID as tagId,\n(select org_code from xip_pub_orgs where org_id = d.org_id) as orgCode,\n(select dept_code from xip_pub_depts where dept_id = t.up_dp_id) as upDeptCode\nFROM xip_pub_depts d\nWHERE CREATION_DATE >= to_date({#lastEtlDate#},'yyyymmddhh24miss')\n   OR LAST_UPDATE_DATE >= to_date({#lastEtlDate#},'yyyymmddhh24miss')","itemId":"sqlswitcher","mysql":"SELECT\nd.DEPT_ID as deptId,\nd.DEPT_CODE as deptCode,\nd.DEPT_NAME as deptName,\nd.DEPT_SHORT_NAME as deptShortName,\nd.UP_DP_ID as upDpId,\nd.ORG_ID as orgId,\nd.ENABLE_FLAG as enableFlag,\nd.ATTRIBUTE1 as attribute1,\nd.ATTRIBUTE2 as attribute2,\nd.ATTRIBUTE3 as attribute3,\nd.ATTRIBUTE4 as attribute4,\nd.ATTRIBUTE5 as attribute5,\nd.ATTRIBUTE6 as attribute6,\nd.ATTRIBUTE7 as attribute7,\nd.ATTRIBUTE8 as attribute8,\nd.ATTRIBUTE9 as attribute9,\nd.ATTRIBUTE10 as attribute10,\nDATE_FORMAT(d.CREATION_DATE,'%Y-%m-%d %H:%i:%s') as creationDate,\nd.CREATED_BY as createdBy,\nDATE_FORMAT(d.LAST_UPDATE_DATE,'%Y-%m-%d %H:%i:%s') as lastUpdateDate,\nd.LAST_UPDATED_BY as lastUpdatedBy,\nd.LAST_UPDATE_LOGIN as lastUpdateLogin,\nd.SEQ_UPDEPT as seqUpdept,\nd.TAG_ID as tagId,\n(select org_code from xip_pub_orgs where org_id = d.org_id) as orgCode,\n(select dept_code from xip_pub_depts where dept_id = t.up_dp_id) as upDeptCode\nFROM xip_pub_depts d\nWHERE CREATION_DATE >= DATE_FORMAT({#lastEtlDate#}, '%Y%m%d%H%i%s')\n   OR LAST_UPDATE_DATE >= DATE_FORMAT({#lastEtlDate#}, '%Y%m%d%H%i%s')"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"autoPage":"false","sql":"{#sqlswitcher#}","createColumns":"false","itemId":"dataprovider1","createKeyValues":"false"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}