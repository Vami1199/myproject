{"inframe":false,"title":"查询所有用户信息","hidden":false,"roles":{"default":1},"children":[{"configs":{"itemId":"module"},"children":[{"configs":{"oracle":"SELECT * FROM (\n    SELECT t1.*, ROWNUM as \"RN\" FROM (\n      select xu.USER_ID,xu.USER_NAME,xe.EMP_ID,xe.EMP_CODE,xe.EMP_NAME, xo.ORG_NAME ,xu.START_DATE,xu.END_DATE\n      from xip_pub_users xu, xip_pub_emps xe ,\n          xip_pub_emp_asg asg left join xip_pub_orgs xo on asg.ORG_ID = xo.ORG_ID\n      where xu.EMP_ID = xe.EMP_ID\n        and xe.EMP_ID = asg.EMP_ID \n        and xu.START_DATE <= sysdate\n        and (xu.END_DATE is null or xu.END_DATE >= sysdate)\n        and (xe.EMP_CODE like '%{#textStr#}%' or xe.EMP_NAME like '%{#textStr#}%' or xu.USER_NAME like '%{#textStr#}%')\n        and xu.USER_ID <> decode(NVL({?exUserId?},''),'','-1',{?exUserId?})\n    ) t1 where ROWNUM<={?bigint.endIndex?}\n  )\nWHERE RN >= {?bigint.beginIndex?}","itemId":"sqlswitcher1","mysql":"select xu.USER_ID,xu.USER_NAME,xe.EMP_ID,xe.EMP_CODE,xe.EMP_NAME, xo.ORG_NAME ,xu.START_DATE,xu.END_DATE\nfrom xip_pub_users xu, xip_pub_emps xe ,\n\txip_pub_emp_asg asg left join xip_pub_orgs xo on asg.ORG_ID = xo.ORG_ID\nwhere xu.EMP_ID = xe.EMP_ID\n  and xe.EMP_ID = asg.EMP_ID \n  and xu.START_DATE <= CURDATE()\n  and (IFNULL(xu.END_DATE,'') = '' or xu.END_DATE >= CURDATE())\n  and (xe.EMP_CODE like '%{#textStr#}%' or xe.EMP_NAME like '%{#textStr#}%' or xu.USER_NAME like '%{#textStr#}%')\n  and xu.USER_ID <> IF(IFNULL({?exUserId?},'')='','-1',{?exUserId?})\nORDER BY xe.EMP_CODE  LIMIT {?bigint.start?},{?bigint.limit?}"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"autoPage":"false","sql":"{#sqlswitcher1#}","totalSql":" select count(1)\n\tfrom xip_pub_users xu, xip_pub_emps xe ,\n\t\txip_pub_emp_asg asg where xu.EMP_ID = xe.EMP_ID and xe.EMP_ID = asg.EMP_ID \n       and (xe.EMP_CODE like '%{#textStr#}%' or xe.EMP_NAME like '%{#textStr#}%' )","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"e