{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"select * \n  from xip_pub_orgs g \n where (g.org_name like '%{#orgname#}%' or g.org_name is null)\n   and g.org_id in (\n     select s.org_id \n       from xip_pub_org_sec s\n      where s.TARGET_ID = {?XIP.userId?}\n        and s.TYPE = 'U'\n     \n     union \n     \n     select s1.org_id \n       from xip_pub_org_sec s1\n      where s1.TARGET_ID = {?funId?}\n        and s1.TYPE = 'F'\n     \n     union \n     \n     select s2.org_id \n       from xip_pub_org_sec s2\n      where s2.TARGET_ID = {?XIP.roleId?}\n        and s2.TYPE = 'D'\n     \n     union \n     \n     select s3.org_id \n       from xip_pub_org_sec s3\n      where s3.TARGET_ID = {?appId?}\n        and s3.TYPE = 'A'\n   )","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}