{"inframe":false,"title":"查询员工用户信息","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT XU.USER_ID,\n       XU.USER_NAME,\n       XE.EMP_ID,\n       XE.EMP_CODE,\n       XE.EMP_NAME,\n       XO.ORG_ID,\n       XO.ORG_NAME\n  FROM XIP_PUB_USERS   XU,\n       XIP_PUB_EMPS    XE,\n       XIP_PUB_ORGS    XO,\n       XIP_PUB_EMP_ASG ASG\n WHERE XU.EMP_ID = XE.EMP_ID\n   AND XO.ORG_ID = ASG.ORG_ID\n   AND XE.EMP_ID = ASG.EMP_ID\n   AND XO.ENABLE_FLAG = 'Y'\n   AND ASG.ENABLED = 'Y'\n   AND XE.EMP_NAME LIKE '%{#empName#}%'\nORDER BY XE.EMP_CODE","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}