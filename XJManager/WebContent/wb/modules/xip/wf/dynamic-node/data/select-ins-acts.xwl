{"inframe":false,"title":"查询流程实例动态节点","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"select a.ACT_CATEGORY,\n       a.ACT_ID,\n       a.PROCESS_ID,\n       a.ACT_CODE,\n       a.ACT_NAME,\n       CONCAT(a.ACT_CODE,a.ACT_NAME) ACT_CODE_NAME,\n       a.ACT_NUM,\n       p.INSTANCE_ID,\n       p.PROPERTY_CODE,\n       p.PROPERTY_NAME,\n       p.PROPERTY_VALUE\n  from xip_wf_ins_acts a, xip_wf_ins_act_properties p\n where a.INSTANCE_ID = p.INSTANCE_ID\n   and a.ACT_ID = p.ACT_ID\n   and p.INSTANCE_ID = {?instanceId?}\n   and p.PROPERTY_CODE = 'a_is_dynamic_node'\n   and a.ACT_CATEGORY = 'Notice'\n   and exists (select 1\n          from xip_wf_ins_act_properties ap\n         where a.INSTANCE_ID = ap.INSTANCE_ID\n           and a.ACT_ID = ap.ACT_ID\n           and ap.PROPERTY_CODE = 'a_is_dynamic_node'\n           and ap.PROPERTY_VALUE = 'Y')","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}