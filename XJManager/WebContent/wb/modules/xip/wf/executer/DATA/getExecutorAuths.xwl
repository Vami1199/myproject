{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","serverScript":"//---------------------获取当前模块语言对应的.json文件，放到request中----------------\ncom.xzsoft.xip.platform.util.XipUtil.getCurrentLang(request,\"xip/wf/executer/json/wf_executer\");","itemId":"module"},"children":[{"configs":{"sql":"SELECT M.VALUE_ID,\n       M.ACT_ID,\n       M.PROPERTY_CODE,\n       M.EXECUTOR_TYPE,\n       CASE\n           WHEN M.EXECUTOR_TYPE = 'U' THEN\n            '{#wf_executer_type_u#}'\n           WHEN M.EXECUTOR_TYPE = 'P' THEN\n            '{#wf_executer_type_p#}'\n           WHEN M.EXECUTOR_TYPE = 'D' THEN\n            '{#wf_executer_type_d#}'\n        END AS EXECUTOR_TYPE_VIEW,\n       M.EXECUTOR_VALUE,\n       CASE\n           WHEN M.EXECUTOR_TYPE = 'U' THEN\n            (SELECT V1.USER_NAME\n               FROM XIP_PUB_USERS V1\n              WHERE V1.USER_ID = M.EXECUTOR_VALUE)\n           WHEN M.EXECUTOR_TYPE = 'P' THEN\n            (SELECT V2.POST_NAME\n               FROM XIP_PUB_POSTS V2\n              WHERE V2.POST_ID = M.EXECUTOR_VALUE)\n           WHEN M.EXECUTOR_TYPE = 'D' THEN\n            (SELECT V3.DEPT_NAME\n               FROM XIP_PUB_DEPTS V3\n              WHERE V3.DEPT_ID = M.EXECUTOR_VALUE)\n        END AS EXECUTOR_VALUE_VIEW\n  FROM XIP_WF_ACT_PV_ENUM M\n WHERE M.ACT_ID = '{#actId#}'\n   AND M.PROPERTY_CODE = 'a_executor_type'\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}